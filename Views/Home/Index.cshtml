@model IEnumerable<GenTestCase.Models.TestCase>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubsystemId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EquipmentId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestPhaseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestCaseId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subsystem.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Equipment.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TestPhase.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TestCaseId)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.TestCaseId })
        </td>
    </tr>
}

</table>
